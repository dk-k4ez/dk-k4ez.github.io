<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>LIST</title>
    <link rel="stylesheet" href="/css/style.css">
    <script src="/send_url.js"></script>
  </head>
  <body>
    <header>
      <a class="header-logo">LIST</a>
    </header>
    <div class="container">
      <div class="container-header">
        <h1>Daftar Belanjaan</h1>
      </div>
      <div class="index-table-wrapper">
        <div class="table-head">
          <span class="id-column">ID</span>
          <span>Item</span>
        </div>
        <!-- Definisikan array item -->
        <% const items = [
          {id: 1, name: 'kentang'},
          {id: 2, name: 'wortel'},
          {id: 3, name: 'bawang'}
        ]; %>
        
        <ul class="table-body">
          <!-- Hapus tiga element <li> dibawah -->
          
          <!-- Hapus sampai disini -->
          
          <!-- Gunakan forEach untuk mencetak elements di array item -->
          <% items.forEach((item) => { %>
  // HTML
  <li>
              <span class="id-column">
                <!-- tampilkan property id milik item-->
                <%= item.id %>
              </span>
              <span class="name-column">
                <!-- tampilkan property name milik item --> 
                <%= item.name %>
              </span>
            </li>
<% }); %>
          
        </ul>
      </div>
    </div>
  </body>
</html>